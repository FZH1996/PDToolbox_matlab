%clear
%load('running-time_pc_lab.mat')
%load('running-time.mat')

cc=hsv(2);

%len = length(run_time);
len = 16;



figure(20)
clf
hold on
plot(fib(1:len), U_eff_avg(:), 'color', cc(1, :))
plot(fib(1:len), U_ineff_avg(:), 'color', cc(1, :))

title('Society`s Surplus')
ylabel('Time')
xlabel('Population Size')


h = legend( 'Optimal Outcome', 'Suboptimal Outcome',4);
set(h,'Interpreter','none')
set(gca, 'YScale', 'log')




figure(21)
clf
hold on
plot(fib(1:len), U_eff_avg(:), 'color', cc(1, :))
plot(fib(1:len), U_ineff_avg(:), 'color', cc(1, :))

title('Society`s Surplus')
ylabel('Time')
xlabel('Population Size')


